{
  "name": "Kodem Cards",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "dotenv -e .env.local -- vite --host 0.0.0.0",
    "build": "tsc && vite build",
    "build:production": "export NODE_ENV=production && tsc && vite build --mode production",
    "deploy": "firebase deploy",
    "deploy:hosting": "firebase deploy --only hosting",
    "deploy:rules": "firebase deploy --only firestore:rules",
    "analyze:deps": "node scripts/analyze-dependencies.js",
    "emulators": "firebase emulators:start",
    "seed:users": "bun run scripts/seed-firebase-users.ts",
    "seed:cards": "bun run scripts/seed-firebase-cards.ts",
    "seed:all": "bun run scripts/seed-firebase-users.ts && bun run scripts/seed-firebase-cards.ts",
    "migrate:deckslots": "bun run scripts/migrate_deckslots.ts",
    "lint": "bun eslint 'src/**/*.{ts,tsx}' --max-warnings=0",
    "lint:fix": "bun eslint 'src/**/*.{ts,tsx}' --fix",
    "lint:report": "bun eslint 'src/**/*.{ts,tsx}' --quiet --no-error-on-unmatched-pattern --output-file=reports/eslint-report.json --format=json",
    "lint:check": "bun eslint 'src/**/*.{ts,tsx}' --max-warnings=1000",
    "clean:unused": "node scripts/fix-lint-issues.js",
    "clean:hooks": "node scripts/fix-react-hooks.js",
    "clean:code": "node scripts/clean-code.js",
    "download-cards": "bun run scripts/download-cards.ts",
    "tag": "bun run scripts/create-tag.js",
    "tag:create": "bun run scripts/create-tag.js --create",
    "tag:push": "bun run scripts/create-tag.js --create --push",
    "tag:push:force": "bun run scripts/create-tag.js --create --push --force",
    "release": "bun run scripts/create-tag.js --release"
  },
